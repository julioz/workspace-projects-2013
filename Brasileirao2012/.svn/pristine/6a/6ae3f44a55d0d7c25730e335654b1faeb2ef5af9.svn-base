<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:actionbar="http://schemas.android.com/apk/res/br.com.zynger.brasileirao2012"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="@drawable/backrepeat"
    android:orientation="vertical">
    <br.com.zynger.brasileirao2012.view.CustomActionBar
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
	    android:id="@+settings/actionbar"
	    actionbar:titleText="@string/settingsactivity_title"
	    actionbar:logosrc="@drawable/ic_settings" />
    <ListView
        android:id="@android:id/list"
        android:cacheColorHint="#00000000"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent" />
</LinearLayout>