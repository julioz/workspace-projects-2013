package br.com.zynger.brasileirao2012;

import java.util.Date;
import java.util.List;

import android.graphics.drawable.Drawable;
import android.os.Bundle;
import android.view.Gravity;
import android.view.View;
import android.view.View.OnClickListener;
import android.widget.FrameLayout;
import android.widget.Toast;
import android.widget.ToggleButton;
import android.widget.ZoomButtonsController;
import br.com.zynger.brasileirao2012.model.Stadium;
import br.com.zynger.brasileirao2012.view.StadiumsMapOverlay;
import br.com.zynger.brasileirao2012.view.StadiumsOverlayItem;

import com.google.android.maps.GeoPoint;
import com.google.android.maps.MapActivity;
import com.google.android.maps.MapController;
import com.google.android.maps.MapView;
import com.google.android.maps.Overlay;

public class StadiumsMapActivity extends MapActivity {
	
	public static final String LIMITATION = "STADIUM_LIMITED";
	
	private String stadium_limited;
	
	private MapView mapView;
	private ToggleButton satelliteToggle;
	private Drawable drawable;
	private List<Overlay> mapOverlays;
	private StadiumsMapOverlay<StadiumsOverlayItem> itemizedOverlay;
	
	@Override
	protected void onCreate(Bundle savedInstanceState) {
		super.onCreate(savedInstanceState);
		setContentView(R.layout.stadiums_map);
		mapView = (MapView) findViewById(R.stadiums_map.mapview);
		satelliteToggle = (ToggleButton) findViewById(R.stadiums_map.satellite_toggle);
		
		stadium_limited = getIntent().getStringExtra(LIMITATION);
		
		mapView.setSatellite(false);
		satelliteToggle.setChecked(false);
		
		satelliteToggle.setOnClickListener(new OnClickListener() {
			@Override
			public void onClick(View v) {
				mapView.setSatellite(!mapView.isSatellite());
			}
		});
		
		mapOverlays = mapView.getOverlays();
		
		MapController mc = mapView.getController();
    	mc.setZoom(5);
    	mc.animateTo(new GeoPoint((int) (-15.783611*1E6), (int) (-47.899167*1E6)));
		
		mapView.setBuiltInZoomControls(true);
    	ZoomButtonsController zbc = mapView.getZoomButtonsController();
    	View zc = zbc.getZoomControls();
    	
    	FrameLayout.LayoutParams lp = (FrameLayout.LayoutParams) zc.getLayoutParams();
    	lp.gravity = Gravity.RIGHT | Gravity.BOTTOM;
	    zc.requestLayout();

	    drawable = getResources().getDrawable(R.drawable.ic_stadium);
	    itemizedOverlay = new StadiumsMapOverlay<StadiumsOverlayItem>(drawable, mapView);

	    populateStadiumsOverlay();
	    
	    if(itemizedOverlay.size() < 1){
	    	Toast.makeText(this, "Não foi possível encontrar o estádio", Toast.LENGTH_SHORT).show();
	    	finish();
	    }

	    mapOverlays.add(itemizedOverlay);
	}
	
	private void populateStadiumsOverlay() {
		Stadium serradourada = new Stadium(-16.698889, -49.233889, "Estádio do Governo do Estado de Goiás", "Serra Dourada", "Goiânia, GO", 50049, new Date(1975, 2, 9), 111, 75, "http://www.ogol.com.br/img/estadios/905/7905_ori_serra_dourada.jpg", "O Estádio do Governo do Estado de Goiás, popularmente conhecido como Estádio Serra Dourada, é um estádio desportivo localizado em Goiânia, Goiás, Brasil.\n\nNo estádio realizam-se as partidas dos principais times de futebol da cidade – Atlético Goianiense, Goiás, Vila Nova e Goiânia –, além de outros eventos em geral, tais como shows musicais.\n\nFoi projetado por Paulo Mendes da Rocha, arquiteto vencedor do Prémio Pritzker de 2006. A sua inauguração ocorreu em 9 de março de 1975. É o sétimo maior e o sétimo melhor estádio do país. Possui um amplo estacionamento para carros, com uma capacidade estimada de 10 mil vagas. A área do estádio é cercada por importantes vias de trânsito, como a rodovia BR-153, Avenida Fuad José Sebba."); //series a e b
	    Stadium independencia = new Stadium(-19.908333, -43.917778, "Estádio Raimundo Sampaio", "Arena Independência", "Belo Horizonte, MG", 23018, new Date(1950, 5, 25), 110, 75, "http://turmadochapeu.com.br/wp-content/uploads/2012/02/independencia1.jpg?e6d64f", "O Estádio Raimundo Sampaio, mais conhecido como Independência, fica no bairro do Horto, em Belo Horizonte, Minas Gerais, Brasil e foi inaugurado em 1950 para a Copa do Mundo de futebol realizada no Brasil. Inicialmente com capacidade para 30.000 pessoas, hoje possui capacidade para 23.000 pessoas. Pertencia ao licenciado Sete de Setembro Futebol Clube, razão pela qual o estádio é chamado de Independência. O nome original é uma homenagem a um ex-presidente do Sete de Setembro.\n\nEm dezembro de 2008, o Governo de Minas Gerais anunciou a reforma no estádio em um convênio celebrado com o América, que transfere a administração do estádio para a Ademg por 20 anos. O investimento inicial anunciado, foi de R$13 milhões de reais por parte do governo do estado e R$ 31 milhões do governo federal. Em agosto de 2009 o Conselho do Meio Ambiente de Belo Horizonte concedeu a licença ambiental para a reforma.\n\nAs obras de reforma tiveram início em 22 de janeiro de 2010, com quase um ano de atraso. A data de entrega da obra, que deveria ser até setembro de 2010, também foi alterada, passando para março de 2011. Todo o estádio foi demolido, com exceção dos vestiários.\n\nEm 25 de novembro de 2010, foram firmados os primeiros pilares do estádio. O custo da obra aumentou para 70 milhões de reais. Em 2011, problemas do financiamento - com o orçamento inflado para R$ 125 milhões - começaram a ser resolvidos, e a reinauguração prevista para dezembro de 2011. Segundo o projeto, o estádio teria capacidade para 25 mil torcedores, entretanto, o número foi reduzido para aproximadamente 23.000 lugares.\n\nPara manutenção do novo estádio, o Governo do Estado abriu licitação à iniciativa privada, que foi vencida em dezembro de 2011 pelo grupo paulista BWA. No dia 14 de fevereiro de 2012, no entanto, a empresa assinou contrato de parceria com o Atlético válido por dez anos, garantindo 45% da receita comercial do estádio. Após questionamentos do Ministério Público, no dia 24 foram realizadas alterações no texto do contrato para que não ocorram duplas interpretações, de maneira a garantir que a administração seja realizada exclusivamente pela BWA. Desta forma, o Clube Atlético Mineiro continua a receber 45% de todos os valores arrecadados pela arena. Os demais 55% pertencem a BWA (45%), América (5%) e governo de Minas (5%).\n\nO estádio foi reinaugurado no dia 25 de abril de 2012 durante cerimônia em que o governador de Minas Gerais, Antonio Anastasia, entregou a chave simbólica do Independência à Bruno Balsimelli, do consórcio BWA, que passou a administrar o local. A partida inaugural, um amistoso entre América-MG e o Argentinos Juniors, que fez parte das comemorações do centenário do time mineiro, marcou também a ultima partida da carreira do atacante Euller. O pontapé inicial foi dado pelo ex-atacante Jair Bala para 5.998 torcedores presentes. A partida terminou com a vitória do time da casa, por 2 a 1, de virada."); //series a e b
	    Stadium pituaçu = new Stadium(-12.946636, -38.417019, "Estádio Roberto Santos", "Pituaçu", "Salvador, BA", 31677, new Date(1979, 2, 11), 110, 68, "http://upload.wikimedia.org/wikipedia/commons/thumb/f/f6/Est%C3%A1dio_Roberto_Santos_%28Pitua%C3%A7u%29.jpg/320px-Est%C3%A1dio_Roberto_Santos_%28Pitua%C3%A7u%29.jpg", "O Estádio Governador Roberto Santos, ou Estádio Roberto Santos, é um estádio de futebol localizado no bairro de Pituaçu, em Salvador, de propriedade do Estado da Bahia. Possui uma capacidade para 31.677 espectadores (depois da reforma e ampliação) e já foi palco de grandes jogos. É mais conhecido como Estádio Metropolitano de Pituaçu, ou simplesmente Estádio de Pituaçu.\n\nO Estádio ganhou o prêmio de melhor gramado da Série B de 2010 concedido pela Confederação Brasileira de Futebol.\n\nO Estádio de Pituaçu é um dos estádios baianos candidatos a centro oficial de treinamento durante a Copa do Mundo FIFA de 2014, uma vez que a Arena Fonte Nova receberá os jogos.");
	    Stadium engenhao = new Stadium(-22.893172, -43.292269, "Estádio Olímpico João Havelange", "Engenhão", "Rio de Janeiro, RJ", 46931, new Date(2007, 5, 30), 105, 68, "http://upload.wikimedia.org/wikipedia/commons/thumb/9/95/Stitched_003.jpg/640px-Stitched_003.jpg", "O Estádio Olímpico João Havelange, também denominado, a partir de 2010, Stadium Rio, e conhecido popularmente por Engenhão, é um estádio poliesportivo localizado no antigo terreno da Rede Ferroviária Federal, localizado no bairro do Engenho de Dentro, na cidade do Rio de Janeiro. Construído no governo Cesar Maia e de propriedade municipal, mas arrendado pelo Botafogo F.R. no ano de sua inauguração até 2027, com possibilidade de renovação de maneira unilateral por mais 20 anos, portanto até 2047, o local foi levantado para sediar as competições de atletismo e futebol dos Jogos Pan-americanos de 2007. Atualmente, tem capacidade total para mais de 46 mil pessoas sentadas, porém, sofrerá ampliação para 60 mil espectadores visando a realização da Olimpíada de 2016.\n\nConsiderado o estádio mais moderno da América Latina à época de sua construção, seu gramado tem dimensões de 105 x 68 m, onde o Botafogo manda suas partidas.");
	    Stadium pacaembu = new Stadium(-23.54855, -46.665242, "Estádio Municipal Paulo Machado de Carvalho", "Pacaembu", "São Paulo, SP", 40199, new Date(1940, 3, 27), 105, 68, "http://upload.wikimedia.org/wikipedia/commons/thumb/b/be/Pacaembu_em_jogo_do_santos.JPG/320px-Pacaembu_em_jogo_do_santos.JPG", "O Estádio Municipal Paulo Machado de Carvalho, mais conhecido por Estádio do Pacaembu, é um estádio desportivo localizado na praça Charles Miller, no final da avenida Pacaembu, no bairro do Pacaembu, na região central da cidade de São Paulo, no Brasil. Pertence à prefeitura da capital paulista e pode ser utilizado nas partidas de futebol pela maioria das equipes do município, por meio de pagamento de aluguel. O Sport Club Corinthians Paulista é o time que joga com maior frequência no local.");
	    Stadium coutopereira = new Stadium(-25.421389, -49.259444, "Estádio Major Antônio Couto Pereira", "Couto Pereira", "Curitiba, PR", 37182, new Date(1932, 10, 15), 109, 72, "http://upload.wikimedia.org/wikipedia/commons/thumb/8/8b/CP2.jpg/320px-CP2.jpg", "O Estádio Major Antônio Couto Pereira, mais conhecido simplesmente por Couto Pereira, é o maior estádio de futebol do estado do Paraná, e pertence ao Coritiba Foot Ball Club. Está localizado no bairro Alto da Glória em Curitiba. Seus torcedores o chamam carinhosamente de Couto, Alto da Glória ou Gigante de Concreto Armado.\n\nFundado em 15 de novembro de 1932, no jogo inaugural o Coritiba venceu o America-RJ por 4 a 2. Atualmente o estádio conta com capacidade para 37.182 pessoas.");
	    Stadium orlandoscarpelli = new Stadium(-27.585556, -48.586667, "Estádio Orlando Scarpelli", "Orlando Scarpelli", "Florianópolis, SC", 19069, new Date(1960, 8, 13), 100, 65, "http://www.fussballtempel.net/conmebol/BRA/Orlando_Scarpelli_A.jpg", "O Estádio Orlando Scarpelli é um estádio de futebol, pertence ao Figueirense Futebol Clube e está localizado no bairro Estreito, na parte continental da cidade de Florianópolis, no estado de Santa Catarina. É o maior estádio de futebol de Florianópolis.\n\nEstá localizado a 1 km do Corpo de Bombeiros, do Batalhão da Polícia Militar e do Hospital de Florianópolis. Desde 1999, o estádio vem sendo constantemente reformado e com novas instalações sendo agregadas ao patrimônio do clube. Novos vestiários para as divisões de base, alambrados novos, implantação de catracas eletrônicas e cartões indutivos de acesso, pintura das arquibancadas, novos banheiros, reforma dos bares, dos acessos internos e do sistema de iluminação, novas casamatas, são algumas das obras efetuadas.\n\nEm 2005, foram executadas mais obras de melhorias. No início do ano houve a colocação de cadeiras em todo o estádio, e no mês de dezembro foi iniciada a substituição do gramado, assim como modernização dos sistemas de irrigação computadorizada e drenagem.\n\nO estádio leva o nome do empresário e desportista Orlando Scarpelli, que presidiu o Figueirense Futebol Clube nos anos 1940 e doou o terreno onde o estádio está localizado.\n\nEm 2002, a Revista Placar atribuiu o título de \"Caldeirão do Brasil\" ao Estádio Orlando Scarpelli devido ao maior percentual de ocupação entre todos os estádios das equipes que disputaram o Campeonato Brasileiro da Série ‘A’, com 49%.");
	    Stadium olimpico = new Stadium(-30.060556, -51.213333, "Estádio Olímpico Monumental", "Olímpico", "Porto Alegre, RS", 48000, new Date(1954, 8, 19), 105, 68, "http://upload.wikimedia.org/wikipedia/commons/thumb/c/c4/Gremio_Stadium.JPG/320px-Gremio_Stadium.JPG", "O Estádio Olímpico Monumental é um estádio brasileiro de futebol situado na cidade de Porto Alegre, Rio Grande do Sul, de propriedade do Grêmio Foot-Ball Porto Alegrense. Foi inaugurado no dia 19 de setembro de 1954 em um torneio de inauguração.");
	    Stadium beirario = new Stadium(-30.065614, -51.236086, "Estádio José Pinheiro Borda", "Beira-Rio", "Porto Alegre, RS", 65000, new Date(1969, 3, 6), 108, 72, "http://www.internacional.com.br/imagens/conteudo/beirario_aerea_2009_1.jpg", "O Estádio José Pinheiro Borda, também conhecido como Gigante da Beira-Rio é o estádio do clube de futebol Sport Club Internacional e está localizado às margens do Rio Guaíba, em Porto Alegre, no estado brasileiro do Rio Grande do Sul. Seu nome oficial é uma homenagem a um cidadão português que durante muitos anos comandou as obras de construção do estádio, morrendo antes que fossem terminadas. Atualmente, é o terceiro maior estádio particular do país.");
	    Stadium aflitos = new Stadium(-8.040556, -34.896667, "Estádio Eládio de Barros Carvalho", "Aflitos", "Recife, PE", 19800, new Date(1939, 5, 25), 105, 70, "http://www.ogol.com.br/img/estadios/970/51970_ori_eladio_de_barros_carvalho_aflitos_.jpg", "O Estádio Eládio de Barros Carvalho, popularmente conhecido como Estádio dos Aflitos, por estar localizado no bairro dos Aflitos, é o estádio usado pelo Clube Náutico Capibaribe. Foi inaugurado em 25 de junho de 1939. O nome é uma homenagem a Eládio de Barros Carvalho, presidente do clube durante 14 mandatos.");
	    Stadium arenabarueri = new Stadium(-23.513186, -46.899436, "Dr. Orlando Batista Novelli", "Arena Barueri", "Barueri, SP", 35000, new Date(2007, 4, 26), 107, 70, "http://upload.wikimedia.org/wikipedia/commons/thumb/1/19/Arena_Barueri.jpg/320px-Arena_Barueri.jpg", "A Arena Barueri, é uma arena multiuso, localizada na cidade de Barueri, Região Metropolitana de São Paulo. A arena pertence a prefeitura municipal de Barueri, e tem como equipe mandante o Sport Club Barueri e agora conta com a volta do Grêmio Barueri. A arena serve também como alternativa para os grandes clubes da capital, devido aos baixo custo de aluguel cobrado pela prefeitura, pelo excelente estado do gramado e o fácil acesso a cidade."); //series a e b
	    Stadium moiseslucarelli = new Stadium(-22.913611, -47.051389, "Estádio Moisés Lucarelli", "Moisés Lucarelli", "Campinas, SP", 19728, new Date(1948, 8, 12), 107, 70, "http://classicoeclassico.sites.uol.com.br/templos/sp/PontePreta8.jpg", "O Estádio Moisés Lucarelli é o estádio pertencente à Associação Atlética Ponte Preta. Localiza-se na cidade de Campinas, no interior do estado de São Paulo, Brasil. Foi inaugurado em 12 de setembro de 1948 com capacidade para 35 mil espectadores, tendo sido construído com doações de material feitas por aficionados do clube. A construção levou seis anos.\n\nAtualmente, teve a capacidade diminuída para cerca de 20 mil pessoas, a fim de proporcionar maior conforto e obedecer às novas determinações legais.\n\nÉ possível que o seu recorde de público tenha sido no jogo entre Ponte Preta e Santos, em 16 de agosto de 1970, quando 33 500 espectadores pagaram ingressos para ver a vitória dos visitantes por 1 a 0. Porém, segundo historiadores, havia cerca de 40 mil torcedores dentro do estádio e mais quatro mil pessoas do lado de fora, sem conseguir entrar. No final desse campeonato paulista, a Ponte Preta conquistou o vice-campeonato.\n\nOficialmente, o maior público é da derrota por 3 a 1 da Ponte Preta para o São Paulo, em 1.º de fevereiro de 1978: 37 274 (34 985 pagantes).");
	    Stadium caninde = new Stadium(-23.520525, -46.618708, "Estádio Doutor Osvaldo Teixeira Duarte", "Canindé", "São Paulo, SP", 21004, new Date(1956, 10, 11), 106, 70, "http://www.ogol.com.br/img/estadios/750/39750_ori_dr_osvaldo_teixeira_duarte_caninde_.jpg", "O Estádio do Canindé (oficialmente Estádio Doutor Osvaldo Teixeira Duarte) é um estádio de futebol localizado às margens do Rio Tietê, na cidade de São Paulo e cuja propriedade é da Associação Portuguesa de Desportos, clube social-poliesportivo ligado à colônia portuguesa da capital paulista.");
	    Stadium vilabelmiro = new Stadium(-23.951111, -46.338889, "Estádio Urbano Caldeira", "Vila Belmiro", "Santos, SP", 21256, new Date(1916, 9, 22), 105, 70, "http://rennan.files.wordpress.com/2007/05/vilabelmiro7.jpg", "O Estádio Urbano Caldeira, mais conhecido como Vila Belmiro, é o maior estádio de futebol da Baixada Santista e abriga o Santos FC, um clube de futebol no Brasil, que foi fundado em 14 de abril de 1912 e revelou o Edson Arantes do Nascimento, conhecido mundialmente por Pelé.\n\nO estádio na Vila Belmiro foi construído em 1916 e é um dos estádios mais antigos do Brasil e tem capacidade atual para 21.256 torcedores, após uma ampla reforma. Está localizado na Rua Princesa Isabel, 77, perto de onde encontra-se o Estádio Ulrico Mursa, da Portuguesa Santista.");
	    Stadium morumbi = new Stadium(-23.600114, -46.720094, "Estádio Cícero Pompeu de Toledo", "Morumbi", "São Paulo, SP", 72809, new Date(1960, 9, 2), 108, 72, "http://upload.wikimedia.org/wikipedia/commons/0/0d/Sao_paulo_e_juventude_-_campeonato_brasileiro_de_2006_-_01.jpg", "O Estádio Cícero Pompeu de Toledo, também chamado de Estádio do Morumbi, é um estádio construído para receber jogos de futebol, sendo a sede oficial do time brasileiro de futebol São Paulo Futebol Clube, já tendo recebido a Seleção Brasileira em várias ocasiões. O Morumbi é o terceiro maior estádio do Brasil - superado apenas pelo Maracanã e pelo Mineirão - e o maior estádio particular do país.");
	    Stadium ilhadoretiro = new Stadium(-8.062778, -34.902778, "Estádio Adelmar da Costa Carvalho", "Ilha do Retiro", "Recife, PE", 35000, new Date(1937, 6, 4), 105, 78, "http://ibxk.com.br/materias/39110949/42230.jpg", "O Estádio Adelmar da Costa Carvalho, mais conhecido como Ilha do Retiro, é um estádio destinado à prática de futebol, e sede oficial do Sport Club do Recife. Localiza-se no bairro da Ilha do Retiro, cidade do Recife, capital de Pernambuco.\n\nFoi inaugurada no dia 4 de julho de 1937, num amistoso entre Sport e Santa Cruz, com vitória do clube rubro-negro pelo placar de 6x5, tendo como autor do primeiro gol o leonino Artur Danzi.\n\nAtualmente, a Ilha tem capacidade máxima para abrigar 35.000 pessoas e é conhecida pela forte pressão causada pela torcida rubro-negra pernambucana em suas arquibancadas, consideradas de boa acústica.\n\nO maior público registrado no Caldeirão da Ilha foi na final do Pernambucano de 1998, entre Sport e Porto, com resultado final de 2x0 para o Papai da Cidade e um número recorde de 56.875 torcedores.\n\nA Ilha Mais Famosa do Brasil destaca-se por ser o primeiro estádio do Norte-Nordeste do país a sediar uma partida de Copa do Mundo, realizada em 1950, entre as seleções do Chile e dos Estados Unidos, e vencida pelos chilenos por 5x2.");
	    Stadium saojanuario = new Stadium(-22.890833, -43.228056, "Estádio Vasco da Gama", "São Januário", "Rio de Janeiro, RJ", 24584, new Date(1927, 3, 21), 115, 85, "http://www.supervasco.com/fotos/sao-januario-5933.jpg", "O Estádio Vasco da Gama, mais conhecido como São Januário, devido à parte de sua localização estar na rua de mesmo nome, é o estádio de futebol pertencente ao Club de Regatas Vasco da Gama. Foi inaugurado em 21 de abril de 1927, sendo até hoje o maior estádio particular do estado do Rio de Janeiro. Sua fachada, em estilo neocolonial, é tombada pelo Instituto do Patrimônio Histórico e Artístico Nacional. Além disso há um Projeto de Lei 1563/96 na Câmara Municipal do Rio de Janeiro, apresentado em 29 de maio de 1996, com o fim de tombar o estádio e seu complexo esportivo e social, por seu valor histórico, cultural, esportivo e social.");
	    Stadium arenadojacare = new Stadium(-19.478513, -44.221827, "Estádio Joaquim Henrique Nogueira", "Arena do Jacaré", "Sete Lagoas, MG", 18850, new Date(2006, 1, 28), 105, 68, "http://upload.wikimedia.org/wikipedia/pt/5/52/Arena_do_jacar%C3%A9.jpg", "Antigamente o Democrata usava o Estádio José Duarte de Paiva com capacidade para 2.000 pessoas. Com a posse de Humberto Timo em 2003, o antigo sonho se tornou realidade. Com pouco dinheiro, o Democrata conseguiu construir o estádio para 18.000 pessoas em um ano, ficando pronto para o segundo jogo do Campeonato Mineiro de 2006.\n\nO estádio foi inaugurado em 28 de janeiro de 2006 na vitória de 3 a 0 sobre o Atlético Mineiro. O primeiro gol foi marcado pelo jogador do Democrata, Paulo César, aos 15 minutos do primeiro tempo.\n\nO jogo com maior público foi Democrata Futebol Clube x Clube Atlético Mineiro, válido pela 1ª rodada do Campeonato Mineiro de 2008. O resultado foi de 1 a 0 (gol de Tuta) para o Democrata, com público de 20.500 pessoas.\n\nEm 2010, com a reforma do Estádio do Mineirão para a Copa do Mundo de 2014, a Arena do Jacaré passou a receber os jogos dos times da capital mineira. Para tanto, o estádio passou por um período de reformas, que tiveram início ainda em 2009. Cadeiras foram instaladas em todo estádio, a capacidade foi aumentada, o estacionamento expandido, construídas novas cabines de imprensa, portões de entrada e saída, iluminação que permitiu jogos noturnos. A reinauguração aconteceu no dia 15 de Julho de 2010, com a vitória do Atlético-MG sobre o Atlético-GO por 3 a 2.");
	    
	    Stadium frasqueirao = new Stadium(-5.889170, -35.179499, "Estádio Maria Lamas Farache", "Frasqueirão", "Natal, RN", 18000, new Date(2006, 1, 22), 105, 68, "http://profsezimar.com/wp-content/uploads/2011/09/frasqueirao-campo-do-abc.jpg", "O Estádio Maria Lamas Farache, apelidado de Frasqueirão, está localizado na Rota do Sol em Natal, capital do Rio Grande do Norte, Brasil. É o estádio oficial do ABC Futebol Clube sendo inaugurado em 22 de janeiro de 2006, no jogo ABC 1 x 1 Alecrim. O estádio ocupa 25.000 m² de um total de 110.000 m² da Vila Olímpica Vicente Farache.\n\nO projeto setoriza a Vila Olímpica em quatro: o estádio com 25.000 m², o estacionamento com 25.000 m², uma área comercial com 14.500 m² e o clube com 45.500 m².");
	    Stadium nazarenao = new Stadium(-6.281643, -35.205082, "Estádio José Nazareno do Nascimento", "Nazarenão", "Goianinha, RN", 6500, new Date(2007, 6, 19), 105, 74, "http://blog.tudonahora.com.br/amigaodagalera/files/2011/09/NAZARENAO12.jpg", "O Estádio José Nazareno do Nascimento, mais conhecido como Nazarenão, é o principal estádio de futebol da cidade de Goianinha, cidade do estado do Rio Grande do Norte. Recebe partidas do time da cidade pelo Campeonato Potiguar e Campeonato Brasileiro de Futebol.\n\nCom a escolha da Cidade de Natal pra sediar a Copa do mundo de 2014 o Estádio José Nazareno do Nascimento Recebeu as Partidas do América de Natal na Série C 2011 onde o América conseguiu o acesso pra Série B e Receberá os jogos do América de Natal na Série B 2012.\n\nO estádio está passando por várias reformas e haverá uma ampliação para 10.000 pessoas.");
	    Stadium fumeirao = new Stadium(-9.748172, -36.666237, "Estádio Municipal Coaracy da Mata Fonseca", "Fumeirão", "Arapiraca, AL", 15000, new Date(1953, 8, 5), 110, 70, "http://www.safern.com/imgs/110729-110001.jpg", "O Estádio Coaracy da Mata Fonseca, também chamado de Fumeirão, é um estádio de futebol da cidade de Arapiraca, no Estado de Alagoas, que atende ao ASA. Sua capacidade é de 15.000 pessoas.\n\nSua construção remonta à década de 50 e na qual recebeu seu nome em homenagem ao prefeito de Arapiraca daquela época, o Dr. Coaracy da Mata Fonseca. Este, por sinal, foi o fundador do ASA, time da crescente Arapiraca e que posteriormente iria competir com os grandes times do Estado de Alagoas, principalmente com os da capital, Maceió.\n\nEm 2010, com a ascensão do time à Série B do Campeonato Brasileiro, o estádio foi devidamente reformado para atender às exigências da competição. Seu projeto de ampliação e modernização foi executado com recursos da prefeitura de Arapiraca mediante repasse da verba de 1,5 milhão de reais.");
	    Stadium vilacapanema = new Stadium(-25.439540, -49.255679, "Estádio Durival Britto e Silva", "Vila Capanema", "Curitiba, PR", 20083, new Date(1947, 1, 23), 110, 70, "http://www.paranaclube.com.br/img/vilacapanema.jpg", "O Estádio Durival Britto e Silva, também conhecido como Estádio Durival Britto, Estádio Durival de Britto ou Vila Capanema, é um estádio de futebol brasileiro localizado em Curitiba e inaugurado em 1947. Foi uma das sedes da Copa do Mundo de 1950, relizada no Brasil, e ao longo de seus mais de 60 anos pertenceu ao Ferroviário, Colorado, e atualmente pertence ao Paraná Clube. Seu nome é uma homenagem ao então Superintendente da Rede Ferroviária do Paraná na época da sua construção, e o apelido vem da sua localização.\n\nEm 2011 o \"Durival Britto\" poderá passar de mãos e fazer parte do patrimônio do Governo do Estado do Paraná, pois desde 1971 existe uma ação judicial pela posse da área pertencente a antiga Rede Ferroviária Federal S.A., aonde encontra-se o estádio. Através de um acordo, esta ação poderá ter fim e o estádio ser entregue, do Governo Federal para o Estadual e assim ser utilizado pela Paraná Clube, por ser o sucessor do Clube Atlético Ferroviário, que foi responsável pela construção do estádio em 1947.");
	    Stadium ressacada = new Stadium(-27.665927, -48.532464, "Estádio Aderbal Ramos da Silva", "Ressacada", "Florianópolis, SC", 17800, new Date(1983, 11, 15), 110, 75, "http://www.fernandodoesse.com.br/wp-content/uploads/2012/02/Foto-ressacada.jpg", "O Estádio Aderbal Ramos da Silva, popularmente conhecido como Estádio da Ressacada, de propriedade do Avaí Futebol Clube, é um estádio de futebol localizado em Florianópolis, capital do estado brasileiro de Santa Catarina. Foi inaugurado em 15 de novembro de 1983, e sua iluminação, em 31 de maio de 1986. Tem capacidade atual para 17.800 pessoas, segundo estimativas recentes, e seu recorde de público é de 25.735 pagantes.\n\nDesde sua inauguração, a Ressacada foi palco de três jogos da Seleção Brasileira e oito finais de campeonatos oficiais.");
	    Stadium melao = new Stadium(-21.569688, -45.415356, "Estádio Municipal Prefeito Dilzon Luiz de Melo", "Melão", "Varginha, MG", 15471, new Date(1988, 10, 7), 110, 75, "http://www.ogol.com.br/img/estadios/614/67614_ori_municipal_prefeito_dilzon_luiz_de_melo_melao_.jpg", "Estádio construído, às pressas, em 1988. Popularmente conhecido como Melão (em decorrência do sobrenome Melo), tem o objetivo de sediar jogos e campeonatos realizados em Varginha, consequentemente projetando o nome dos jogadores varginhenses e suas equipes.\n\nO Melão foi palco do primeiro jogo que a seleção brasileira realizou se preparando para a Copa de 1994, uma vitória sobre a Iugoslávia por 3 x 1. A campanha vitoriosa a partir deste jogo culminou com a conquista do Tetracampeonato Mundial de Futebol.\n\nO Estádio foi construído durante a gestão do Prefeito Dilzon Luiz de Melo, de 1983 a 1988, e em sua homenagem ostenta o seu nome. Este, desde 1991 ocupa o cargo de deputado estadual em Minas Gerais, sendo o atual primeiro secretário da Assembléia Legislativa de Minas Gerais.\n\nFoi construído com um projeto inicial que chegaria a 45.000 lugares, mas após terminado sua capacidade ficou registrada com 20.000 lugares. Pelas atuais normas de segurança sua capacidade é de 15.471 torcedores, e com o Mineirão fechado para reformas ele fica na 5ª posição entre os maiores estádios de Minas Gerais, estando atrás apenas dos estádios Parque do Sabiá em Uberlândia com 50.000 lugares, do Helenão em Juiz de Fora com 40.000 lugares, do Ipatingão em Ipatinga com 27.500 lugares e da Arena do Jacaré em Sete Lagoas com 20.063 lugares.");
	    Stadium nabichedid = new Stadium(-22.965536, -46.536970, "Estádio Nabi Abi Chedid", "Nabi Abi Chedid", "Bragança Paulista, SP", 21209, new Date(1965, 1, 18), 110, 68, "http://farm3.static.flickr.com/2757/4380036987_62d68aa73b.jpg", "O Estádio Nabi Abi Chedid é um estádio de futebol localizado na cidade de Bragança Paulista, São Paulo, Brasil. Sua capacidade atual é de 13.212 pessoas. Foi construído em 1949, mas a inauguração oficial deu-se apenas em janeiro de 1965. O estádio é de propriedade do Bragantino.\n\nAté 6 de janeiro de 2009 seu nome era Estádio Marcelo Stéfani (1914—1985), ex-jogador e ex-presidente do clube. No entanto, em janeiro de 2009 a direção do Bragantino decidiu mudá-lo para Estádio Nabi Abi Chedid. Nabi Abi Chedid era o pai do presidente do clube, Marquinho Chedid. A área esportiva que abriga o estádio, então, passou a chamar-se Marcelo Stéfani. A troca de nome foi reprovada por boa parte da população da cidade.");
	    Stadium pv = new Stadium(-3.745978, -38.536837, "Estádio Presidente Vargas", "Presidente Vargas", "Fortaleza, CE", 20166, new Date(1941, 9, 14), 105, 71, "http://upload.wikimedia.org/wikipedia/commons/thumb/a/a6/EstadioPesidenteVargas.JPG/799px-EstadioPesidenteVargas.JPG", "O Estádio Presidente Vargas, mais conhecido como PV, é um estádio de futebol brasileiro localizado em Fortaleza, capital do Estado do Ceará, Brasil. Sedia principalmente os jogos de Ceará, Ferroviário e Fortaleza. Está localizado na rua Marechal Deodoro da Fonseca, 1187, no Benfica.");
	    Stadium reipele = new Stadium(-9.671637, -35.759235, "Estádio Rei Pelé", "Trapichão", "Maceió, AL", 18500, new Date(1970, 10, 25), 110, 75, "http://www.educacao.al.gov.br/configuracao/trapichao/campo_do_Trapichao.JPG/image_preview", "O Estádio Rei Pelé, vulgarmente chamado de Trapichão, foi inaugurado no dia 25 de outubro de 1970 com o jogo amistoso entre Seleção Alagoana e Santos e é utilizado habitualmente pelas equipes alagoanas do CSA e do CRB. Pelé atuou neste jogo e o primeiro gol do Trapichão foi de autoria do santista Douglas. O jogo foi ganho pelo Santos por 5 a 0, com público de 45.865 espectadores, que permanece sendo o recorde de público deste estádio até os dias atuais. Atualmente o estádio tem capacidade para 20.801 pessoas, de acordo com o CNEF editado pela CBF em 14 de agosto de 2009."); //http://pt.wikipedia.org/wiki/Ficheiro:Rei_Pel%C3%A9.jpg
	    Stadium majestoso = new Stadium(-28.683999, -49.367838, "Estádio Heriberto Hülse", "Majestoso", "Criciúma, SC", 22000, new Date(1955, 10, 16), 105, 68, "http://futebolsc.com/imagens/item_7899.jpg", "O estádio do Criciúma Esporte Clube, um dos principais do estado de Santa Catarina, já abrigou competições de nível internacional como a Copa Libertadores da América, época na qual foi completamente adaptado para competição, destaque entre os principais estádios do estado, é o uníco completamente coberto.\n\nO primeiro proprietário do estádio foi o Comerciário Esporte Clube que no ano de 1978 veio se tornar o Criciuma Esporte Clube, atual proprietário do Majestoso.\n\nO estádio atualmente tem capacidade para 22.000 torcedores, pois a capacidade foi adequada para cumprir as normas do estatuto do torcedor.\n\nO maior publico registrado foi em 6 de agosto de 1995 no jogo Criciúma 1 X 0 Chapecoense-SC pelo campeonato catarinense. O jogo teve um público de 31.123 pessoas e uma renda de R$115.815,00.\n\nO nome do estádio é uma homenagem ao ex-governador do estado de Santa Catarina, Heriberto Hülse, por ser uma figura política que representou o sul Catarinense, onde se situa a cidade de Criciúma e por vez o Majestoso.\n\nO apelido de Majestoso foi dado carinhosamente pela torcida do Tigre, pelo devido formato do estádio em tamanho regular e por ter uma aparência imponente, digno do apelido Majestoso.");
	    Stadium brincodeouro = new Stadium(-22.909355, -47.043704, "Estádio Brinco de Ouro da Princesa", "Brinco de Ouro", "Campinas, SP", 32453, new Date(1953, 5, 31), 110, 75, "http://www.blogtorcida.com.br/wp-content/uploads/2010/06/brinco-de-outro.jpg", "O Estádio Brinco de Ouro da Princesa, ou apenas Brinco de Ouro, é o estádio do Guarani Futebol Clube, de Campinas, no interior do estado de São Paulo, Brasil. É o maior estádio da cidade, inaugurado em 31 de maio de 1953. A denominação do estádio ocorreu quando um determinado jornalista presente à apresentação de sua maquete, surpreendeu a todos comparando seu formato a um brinco e associou ao \"apelido carinhoso\" da cidade de Campinas (Princesa D'Oeste).\n\nA iluminação foi inaugurada em 11 de janeiro de 1964, com um jogo amistoso no qual o Guarani venceu o Flamengo por 2 a 1.\n\nSua capacidade atual segundo o CNEF (Cadastro Nacional de Estádios de Futebol) editado pela CBF em 15 de setembro de 2009 é de 32 453 pessoas. O recorde de público do estádio é de 52 002 pessoas, no jogo Guarani versus Flamengo em 1982 (a capacidade divulgada naquela época era de 53 mil pessoas, diminuída para efeito de proporcionar maior conforto e segurança para os espectadores), sendo que até 1978, não possuía o Tobogã (a arquibancada superior), possuindo capacidade para 32 mil pessoas segundo os padrões de medição daquela época, embora, em 26 de novembro de 1975 tenha recebido 34 513 espectadores em partida contra o Fluminense pelo Campeonato Brasileiro.\n\nAs dimensões do gramado de jogo foram aumentadas de 105 x 70m para 110m de comprimento, por 75m de largura, na última reforma do gramado em 2002.");
	    Stadium ipatingao = new Stadium(-19.470861, -42.541358, "Estádio Municipal João Lamego Netto", "Lamegão", "Ipatinga, MG", 16000, new Date(1982, 11, 23), 110, 75, "http://www.galonautas.com.br/wp-content/uploads/Lameg%C3%A3o.jpg", "O Estádio Municipal João Lamego Netto, o Lamegão, é um estádio do estado de Minas Gerais, com capacidade para 16.000 espectadores. Atualmente, é utilizado pelo Ipatinga Futebol Clube.\n\nEm 2010, com o a reforma do Estádio do Mineirão para a Copa do Mundo de 2014, o Ipatingão passou a sediar alguns dos jogos das equipes da capital mineira.\n\nEm 2011, teve seu nome alterado de Estádio Municipal Epaminondas Mendes Brito para Estádio Municipal João Lamego Netto em homenagem a João Lamego Netto, falecido em 4 de abril desse ano, que foi prefeito de Ipatinga de 1977 a 1982, cujo mandato ocorreram várias obras de melhorias na infraestrutura da cidade, além de ter acontecido a construção do Ipatingão e a viabilização do Parque Ipanema");
	    Stadium ninhodagarca = new Stadium(-22.807695, -45.185544, "Estádio Municipal Professor Dario Rodrigues Leite", "Dario Rodrigues Leite", "Guaratinguetá, SP", 15769, new Date(1965, 9, 7), 108, 66, "http://www.ceroacero.es/img/estadios/535/7535_ori_prof_dario_rodrigues_leite_ninho_da_garca_.jpg", "O Estádio Municipal Professor Dario Rodrigues Leite, também conhecido como Ninho da Garça, está localizado na Praça da Bíblia, na cidade de Guaratinguetá, no estado de São Paulo.\n\nInaugurado em 7 de Setembro de 1965 com a partida entre Associação Esportiva Guaratinguetá e Esporte Clube Taubaté, com resultado final de 0x0.\n\nSuas dimensões são de 108 x 66m. Tem capacidade para 15.769 pessoas");
	    Stadium arenajoinville = new Stadium(-26.316027, -48.833596, "Estádio Arena Joinville", "Arena Joinville", "Joinville, SC", 22400, new Date(2004, 9, 24), 105, 70, "http://mavalem.sites.uol.com.br/sc/Arena.jpg", "A Arena Joinville, localizada na cidade de Joinville, é uma arena multiuso municipal e maior estádio de futebol de Santa Catarina além de estar sendo utilizado pelo Joinville Esporte Clube. Localizado no bairro Bucarein, a construção tem design inspirado em várias arenas de futebol européias, como a Amsterdam Arena.Tendo uma grande semelhança com a Arena da Baixada em Curitiba e com a Arena da Floresta em Rio Branco no Acre.\n\nTeve sua primeira etapa inaugurada em 25 de setembro de 2004, na partida entre Joinville e Seleção Masters. A segunda etapa foi inaugurada em 26 de julho de 2007, na partida Joinville 4-3 Atlético Paranaense. Com isso, a capacidade, que inicialmente era de 15.000 pessoas, foi expandida para 22.400, e será novamente expandida para 30 mil pessoas (24 mil sentadas). O custo estimado para a expansão é de R$ 7 milhões. O projeto final prevê que a Arena contará ainda com lojas comerciais, praça de alimentação, estacionamento e parque público anexo.\n\nPor ser um estádio público e não propriedade do Joinville Esporte Clube, ele deverá ter retomadas as obras assim que o governo municipal angariar verbas junto ao governos estadual e federal para conclusão da terceira etapa.");
	    Stadium anacleto = new Stadium(-23.630671, -46.560756, "Estádio Municipal Anacleto Campanella", "Anacleto Campanella", "São Caetano do Sul, SP", 22124, new Date(1955, 1, 2), 110, 75, "http://www.ogol.com.br/img/estadios/831/7831_ori_estadio_municipal_anacleto_campanella.jpg", "O Estádio Municipal Anacleto Campanella é um estádio de futebol brasileiro que pertence à prefeitura de São Caetano do Sul, São Paulo. É utilizado pela Associação Desportiva São Caetano e atualmente tem capacidade para 16.744 pessoas.");
	    Stadium barradao = new Stadium(-12.918671, -38.428116, "Estádio Manoel Barradas", "Barradão", "Salvador, BA", 35632, new Date(1986, 11, 11), 106, 70, "http://upload.wikimedia.org/wikipedia/commons/thumb/d/da/Est%C3%A1dio_Manoel_Barradas_Vit%C3%B3ria.jpg/800px-Est%C3%A1dio_Manoel_Barradas_Vit%C3%B3ria.jpg", "O Estádio Manoel Barradas, o Barradão, é um estádio de futebol de Salvador (Bahia), de propriedade do Esporte Clube Vitória. Foi inaugurado em 11 de novembro de 1986, com a partida Vitória 1 a 1 Santos e reinaugurado em 25 de agosto de 1991, com o jogo Vitória 1 a 1 Olimpia, do Paraguai, campeão da Libertadores do ano anterior. Sua capacidade atual é de pouco mais de 35 mil espectadores.\n\nEm 30 de outubro de 2010, na vitória sobre o Vasco da Gama por 4 a 2, o Vitória completou 500 jogos em seu estádio, com histórico de 316 vitórias, 104 empates e 80 derrotas, tendo marcado 1.117 gols e sofrido 520, com saldo de 581 gols, e aproveitamento superior a 70%. O público médio da história do estádio é de cerca de 10.000 torcedores por jogo ou mais de 13.000 se considerados apenas públicos em Campeonatos Brasileiros.\n\nNa Série C de 2006, a média de público do Vitória foi de 15.062 pagantes por partida. Já na Série B de 2007, foi de mais de 18.000 pagantes. Na edição de 2008, quase 16.000 pessoas estavam no estádio por partida, enquanto que em 2009, com o aumento do preço dos ingressos, a média caiu para pouco menos de 13.400.");
	    Stadium gigantedoitibire = new Stadium(-25.524798, -48.508308, "Estádio Fernando Charbub Farah", "Gigante do Itiberê", "Paranaguá, PR", 12218, new Date(2004, 9, 29), 100, 75, "http://www.esporte.pr.gov.br/modules/galeria/uploads/406/normal_Estadio_Paranagua.JPG", "O Estádio Fernando Charbub Farah, conhecido como Gigante do Itiberê, é um estádio poliesportivo localizado na cidade de Paranaguá, no estado do Paraná, tem capacidade para mais de 12.000 pessoas. Possui pista de atletismo. Foi inaugurado em 2004 no jogo Paraná x Vasco da Gama, com o resultado de 2 x 1 para o Paraná. está localizado na região do centro histórico de Paranaguá.\n\nO primeiro gol na história do estádio foi marcado pelo meio-campista sérvio Dejan Petković, então defendendo o Vasco da Gama.");
	    
