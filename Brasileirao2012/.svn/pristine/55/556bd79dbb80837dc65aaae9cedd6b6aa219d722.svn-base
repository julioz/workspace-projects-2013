<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="CustomActionBar">
        <attr name="titleText" format="string" />
        <attr name="titleSize" format="dimension" />
        <attr name="logosrc" format="reference" />
        <attr name="showTextBox" format="boolean" />
        <attr name="showUpdate" format="boolean" />
        <attr name="showZoom" format="boolean" />
        <attr name="showImage" format="boolean" />
    </declare-styleable>

    <attr name="refreshActionItemStyle" format="reference" />

    <declare-styleable name="RefreshActionItem">
        <attr name="refreshActionItemBackground" format="reference|color" />
        <attr name="refreshActionItemIcon" format="reference" />
    </declare-styleable>

</resources>