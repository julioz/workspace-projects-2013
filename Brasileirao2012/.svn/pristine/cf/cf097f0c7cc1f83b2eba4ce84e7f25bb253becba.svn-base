package br.com.zynger.brasileirao2012.model;

public class NewsSource {
	
	public static final int GLOBOESPORTE = 1;
	public static final int UOL = 3;
	public static final int GOOGLE = 4;
	public static final int NETFLU = 5;
	public static final int PARANAUTAS = 6;
	
	public static final String ENCODING_UTF8 = "utf-8";
	public static final String ENCODING_ISO8859 = "iso-8859-1";
	
	private String name;
	private String url;
	private String encoding;
	private Integer domain;
	
	public NewsSource(String name, String url, String encoding, Integer domain) {
		this.name = name;
		this.url = url;
		this.encoding = encoding;
		this.domain = domain;
	}
	
	public String getName() {
		return name;
	}
	
	public String getUrl() {
		return url;
	}
	
	public String getEncoding() {
		return encoding;
	}
	
	public Integer getDomain() {
		return domain;
	}
}
