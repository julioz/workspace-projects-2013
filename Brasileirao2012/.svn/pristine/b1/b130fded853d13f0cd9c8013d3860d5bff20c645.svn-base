package br.com.zynger.brasileirao2012.model;

import java.io.Serializable;

public class Striker implements Serializable {

	private static final long serialVersionUID = 1L;
	
	private String name, team;
	private int goals;
	
	public Striker() { }
	
	public Striker(String name, String team, int goals) {
		setName(name);
		setTeam(team);
		setGoals(goals);
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getTeam() {
		return team;
	}

	public void setTeam(String team) {
		this.team = team;
	}

	public int getGoals() {
		return goals;
	}

	public void setGoals(int goals) {
		this.goals = goals;
	}

	@Override
	public String toString() {
		return "Striker [name=" + name + ", team=" + team + ", goals=" + goals
				+ "]";
	}
	
	
}
