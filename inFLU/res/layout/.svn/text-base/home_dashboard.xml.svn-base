<?xml version="1.0" encoding="utf-8"?>
<TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:layout_below="@+home/llLogo"
	android:id="@+home/tl_buttons"
	android:weightSum="3">
	
	<TableRow android:layout_width="fill_parent"
		android:layout_height="0dp"
		android:gravity="center"
		android:weightSum="3"
		android:layout_weight="1">
		
		<LinearLayout android:layout_width="0dp"
			android:layout_height="wrap_content"
			android:orientation="vertical"
			android:gravity="center"
			android:layout_weight="1">
			
			<ImageButton android:src="@drawable/ic_ball"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:id="@+home/ibNextGame"
				android:background="@null"/>
				
			<TextView android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:textColor="#FFFFFFFF"
				android:gravity="center_horizontal"
				android:text="Próximo Jogo"
				android:textStyle="bold"/>

		</LinearLayout>
		
		<LinearLayout android:layout_width="0dp"
			android:layout_height="wrap_content"
			android:orientation="vertical"
			android:gravity="center"
			android:layout_weight="1">
			
			<ImageButton android:src="@drawable/ic_news"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:id="@+home/ibNews"
				android:background="@null"/>
				
			<TextView android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:textColor="#FFFFFFFF"
				android:text="Notícias"
				android:textStyle="bold" />

		</LinearLayout>
		
		<LinearLayout android:layout_width="0dp"
			android:layout_height="wrap_content"
			android:orientation="vertical"
			android:gravity="center"
			android:layout_weight="1">
			
			<ImageButton android:src="@drawable/ic_ranking"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:id="@+home/ibRanking"
				android:background="@null"/>
				
			<TextView android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:textColor="#FFFFFFFF"
				android:text="Classificação"
				android:textStyle="bold" />

		</LinearLayout>
	</TableRow>
	
	<TableRow android:layout_width="fill_parent"
		android:layout_height="0dp"
		android:gravity="center"
		android:weightSum="3"
		android:layout_weight="1">
		
		<LinearLayout android:layout_width="0dp"
			android:layout_height="wrap_content"
			android:orientation="vertical"
			android:gravity="center"
			android:layout_weight="1">
			
			<ImageButton android:src="@drawable/ic_shirt"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:id="@+home/ibSquad"
				android:background="@null"/>
				
			<TextView android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:textColor="#FFFFFFFF"
				android:gravity="center_horizontal"
				android:text="Elenco"
				android:textStyle="bold"/>

		</LinearLayout>
		
		<LinearLayout android:layout_width="0dp"
			android:layout_height="wrap_content"
			android:orientation="vertical"
			android:gravity="center"
			android:layout_weight="1">
			
			<ImageButton android:src="@drawable/ic_statistics"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:id="@+home/ibStatistics"
				android:background="@null"/>
				
			<TextView android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:textColor="#FFFFFFFF"
				android:text="Estatísticas"
				android:textStyle="bold"/>

		</LinearLayout>
		
		<LinearLayout android:layout_width="0dp"
			android:layout_height="wrap_content"
			android:orientation="vertical"
			android:gravity="center"
			android:layout_weight="1">
			
			<ImageButton android:src="@drawable/ic_tickets"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:id="@+home/ibTickets"
				android:background="@null"/>
				
			<TextView android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:textColor="#FFFFFFFF"
				android:text="Ingressos"
				android:textStyle="bold"/>

		</LinearLayout>
	</TableRow>
	
	<TableRow android:layout_width="fill_parent"
		android:layout_height="0dp"
		android:gravity="center"
		android:weightSum="3"
		android:layout_weight="1">
		
		<LinearLayout android:layout_width="0dp"
			android:layout_height="wrap_content"
			android:orientation="vertical"
			android:gravity="center"
			android:layout_weight="1">
			
			<ImageButton android:src="@drawable/ic_phrase"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:id="@+home/ibPhrases"
				android:background="@null"/>
				
			<TextView android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:textColor="#FFFFFFFF"
				android:gravity="center_horizontal"
				android:text="Frases"
				android:textStyle="bold"/>

		</LinearLayout>
		
		<LinearLayout android:layout_width="0dp"
			android:layout_height="wrap_content"
			android:orientation="vertical"
			android:gravity="center"
			android:layout_weight="1">
			
			

		</LinearLayout>
		
		<LinearLayout android:layout_width="0dp"
			android:layout_height="wrap_content"
